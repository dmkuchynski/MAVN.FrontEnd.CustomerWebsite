{
  "name": "token-customer-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "node patch.js",
    "ng": "ng",
    "serve-en": "ng serve --configuration=en --host 0.0.0.0 --disable-host-check",
    "serve-de": "ng serve --configuration=de --port=4201",
    "build": "npm run build-all",
    "build-all": "npm run build:en && npm run build:de",
    "build:en": "ng build --configuration=en-prod --localize",
    "build:de": "ng build --configuration=de-prod --localize",
    "i18n": "npm run i18n-extract && npm run i18n-clear",
    "i18n-extract": "ng xi18n --out-file messages.generated.xlf --output-path locale",
    "i18n-clear": "node i18n-clear.js",
    "test": "ng test",
    "hooks:pre-commit": "npm-run-all prettier:fix-staged",
    "hooks:pre-push": "npm-run-all lint",
    "prettier:fix-staged": "pretty-quick --staged",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all hooks:pre-commit"
    }
  },
  "dependencies": {
    "@angular/animations": "^9.1.3",
    "@angular/cdk": "^9.2.1",
    "@angular/common": "^9.1.3",
    "@angular/compiler": "^9.1.3",
    "@angular/core": "^9.1.3",
    "@angular/forms": "^9.1.3",
    "@angular/localize": "~9.1.3",
    "@angular/material": "^9.2.1",
    "@angular/material-moment-adapter": "^9.2.1",
    "@angular/platform-browser": "^9.1.3",
    "@angular/platform-browser-dynamic": "^9.1.3",
    "@angular/router": "^9.1.3",
    "@types/web3": "1.0.19",
    "core-js": "2.5.4",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "ngx-api-utils": "^1.0.8",
    "node-sass": "^4.13.1",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "web3": "1.2.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "^9.1.3",
    "@angular/language-service": "^9.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.10.0",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.8.3"
  }
}
